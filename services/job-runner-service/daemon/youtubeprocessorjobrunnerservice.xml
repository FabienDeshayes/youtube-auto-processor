<service>
	<id>youtubeprocessorjobrunnerservice.exe</id>
	<name>Youtube Processor Job Runner Service</name>
	<description>This service run every minute, parse jobs in the database and manage them</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>Z:\Dev\workspace\youtube-auto-processor\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>Z:\Dev\workspace\youtube-auto-processor\services\job-runner-service\job-runner.js</argument>
	<argument>--log</argument>
	<argument>Youtube Processor Job Runner Service wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<env name="HOME" value="C:\Users\Jérémy"/>
	<workingdirectory>Z:\Dev\workspace\youtube-auto-processor</workingdirectory>
</service>